{
	"name": "tribal",
	"version": "0.0.1",
	"author": "stretch1414",
	"description": "An initial framework boilerplate for projects",
	"repository": {
		"url": "git+https://github.com/stretch1414/tribal.git"
	},
	"bugs": {
		"url": "https://github.com/stretch1414/tribal/issues"
	},
	"homepage": "https://github.com/stretch1414/tribal#readme",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"type": "module",
	"engines": {
		"bun": "1.3.5"
	},
	"scripts": {
		"build": "bunx tsc",
		"dev": "bun --watch server.ts | pino-pretty",
		"start": "bun dist/server.js",
		"db:migrate:dev": "bunx prisma migrate dev",
		"db:migrate:prod": "bunx prisma migrate deploy",
		"db:seed": "bunx prisma db seed"
	},
	"dependencies": {
		"@prisma/adapter-pg": "7.2.0",
		"@prisma/client": "7.2.0",
		"@react-email/components": "1.0.4",
		"@react-email/render": "2.0.4",
		"argon2": "0.44.0",
		"close-with-grace": "2.4.0",
		"cors": "2.8.5",
		"dayjs": "1.11.19",
		"dotenv": "17.2.3",
		"express": "5.2.1",
		"jsonwebtoken": "9.0.3",
		"pino": "10.1.1",
		"pino-http": "11.0.0",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"resend": "6.7.0",
		"stripe": "20.1.2",
		"zod": "4.3.5"
	},
	"devDependencies": {
		"@types/cors": "2.8.19",
		"@types/express": "5.0.6",
		"@types/jsonwebtoken": "9.0.10",
		"@types/react": "19.2.8",
		"bun-types": "1.3.5",
		"pino-pretty": "13.1.3",
		"prettier": "3.7.4",
		"prisma": "7.2.0",
		"typescript": "5.9.3"
	}
}
